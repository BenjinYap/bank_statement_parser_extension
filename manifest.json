{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "storage"
  ]
}
